{
    "name": "Multiple_Choice_Quiz",

    "build": {
        "dockerfile": "Dockerfile",
        "context": "..",
    },

    "extensions": [
        "ms-vscode.cpptools",
        "hediet.vscode-drawio",
    ],

    "runArgs": [
        "-e", "DISPLAY=:0"
    ],
    "mounts": [
        "source=/tmp/.X11-unix/,target=/tmp/.X11-unix,type=bind,consistency=cached"
    ],
}
